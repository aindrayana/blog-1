<div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-4">
    <table>
      <% @posts.each do |post| %>
        <tr>
        <td><h1><%= link_to post.title, post_path(post) %></h1>
                <p class="lead"><%= post.body %></p><hr/>
        </td>
        </tr>
        <tr>
        <td></td>
        </tr>
      <% end %>
    </table>
  </div>
  <div class="col-md-4"></div>
</div>
